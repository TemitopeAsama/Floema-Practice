    "errorhandler": "^1.5.1",
    "express": "^4.20.0",
    "include-media": "^2.0.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "pug": "^3.0.3"
  },
  "type": "module"
}
