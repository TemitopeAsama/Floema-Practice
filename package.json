{
  "name": "floema",
  "version": "1.0.0",
  "description": "A practice website for practicing creative development",
  "scripts": {
    "backend:build": "node app.js",
    "backend:dev": "nodemon app.js",
    "frontend:build": "webpack --progress --config webpack.config.build.js",
    "frontend:dev": "webpack serve --progress --config webpack.config.development.js",
    "start": "concurrently --kill-others \"npm run backend:dev\" \"npm run frontend:dev\""
  },
  "author": "Temitope Asama",
  "license": "ISC",
  "devDependencies": {
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "image-minimizer-webpack-plugin": "^4.1.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^11.0.1",
    "mini-css-extract-plugin": "^2.9.1",
    "nodemon": "^3.1.4",
    "postcss-loader": "^8.1.1",
    "sass": "^1.78.0",
    "sass-loader": "^16.0.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
